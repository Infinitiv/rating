<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">id</Data></Cell>
        <Cell><Data ss:Type="String">last_name</Data></Cell>
        <Cell><Data ss:Type="String">first_name</Data></Cell>
        <Cell><Data ss:Type="String">middle_name</Data></Cell>
	<Cell><Data ss:Type="String">chair_id</Data></Cell>
	<Cell><Data ss:Type="String">post_id</Data></Cell>
	<Cell><Data ss:Type="String">degree_id</Data></Cell>
	<Cell><Data ss:Type="String">academic_title_id</Data></Cell>
	<Cell><Data ss:Type="String">head</Data></Cell>
      </Row>
    <% @employees.each do |employee| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= employee.id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= employee.last_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= employee.first_name %></Data></Cell>
        <Cell><Data ss:Type="String"><%= employee.middle_name %></Data></Cell>
	<Cell><Data ss:Type="Number"><%= employee.chair_id %></Data></Cell>
	<Cell><Data ss:Type="Number"><%= employee.post_id %></Data></Cell>
	<Cell><Data ss:Type="Number"><%= employee.degree_id %></Data></Cell>
	<Cell><Data ss:Type="Number"><%= employee.academic_title_id %></Data></Cell>
	<Cell><Data ss:Type="String"><%= employee.head %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>